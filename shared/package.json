{
  "name": "@karpatkey-monorepo/shared",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:staged": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "generate-cache:dailyBalanceReports": "ts-node --project ./tsconfig.json ./scripts/generateCache.ts --report-name getDailyBalanceReports",
    "generate-cache:treasuryFinancialMetrics": "ts-node --project ./tsconfig.json ./scripts/generateCache.ts --report-name getTreasuryFinancialMetrics",
    "generate-cache:tokens": "ts-node --project ./tsconfig.json ./scripts/generateCache.ts --report-name getTokens",
    "generate-cache:treasuryVariationMetricsDetail": "ts-node --project ./tsconfig.json ./scripts/generateCache.ts --report-name getTreasuryVariationMetricsDetail",
    "generate-cache": "yarn generate-cache:dailyBalanceReports && yarn generate-cache:treasuryFinancialMetrics && yarn generate-cache:tokens && yarn generate-cache:treasuryVariationMetricsDetail",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint './**/*.{ts,tsx}'",
    "lint:eslint:fix": "eslint --fix './**/*.{ts,tsx}'",
    "lint:tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@ethersproject/address": "^5.7.0",
    "@google-cloud/bigquery": "^6.2.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "@mui/utils": "^5.11.13",
    "@types/randomcolor": "^0.5.7",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "commander": "^10.0.0",
    "luxon": "^3.3.0",
    "next": "^13.2.4",
    "numbro": "^2.3.6",
    "randomcolor": "^0.6.2",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/luxon": "^3.2.0",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsc-files": "^1.1.3",
    "typescript": "5.0.2"
  },
  "lint-staged": {
    "./**/*.+(js,ts,tsx)": [
      "eslint -c ./shared/.eslintrc.json -cache --fix",
      "tsc-files --noEmit"
    ],
    "./**/*.{js,ts,tsx,css,md,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  }
}
