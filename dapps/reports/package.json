{
  "name": "@karpatkey-monorepo/reports",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.13.0",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint:staged": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:eslint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:tsc": "tsc -p tsconfig.json",
    "cache:treasuryFinancialMetrics": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getTreasuryFinancialMetrics",
    "cache:tokens": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getTokens",
    "cache:treasuryVariationMetricsDetail": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getTreasuryVariationMetricsDetail",
    "cache:treasuryFinancialPositions": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getTreasuryFinancialPositions",
    "cache:treasuryHistoricVariation": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getTreasuryHistoricVariation",
    "cache:financialMetricAndVarDetail": "ts-node --project ./tsconfig.json ./src/scripts/generateCache.ts --report-name getFinancialMetricAndVarDetail",
    "generate:cache": "yarn cache:treasuryFinancialMetrics && yarn cache:tokens && yarn cache:treasuryVariationMetricsDetail && yarn cache:treasuryFinancialPositions && yarn cache:treasuryHistoricVariation && yarn cache:financialMetricAndVarDetail"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@karpatkey-monorepo/shared": "0.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@mui/x-date-pickers": "^6.7.0",
    "commander": "^10.0.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.8",
    "dotenv": "^16.2.0",
    "framer-motion": "^10.12.16",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.4.0",
    "luxon": "^3.3.0",
    "next": "^13.4.6",
    "numbro": "^2.3.6",
    "puppeteer": "^20.7.2",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-intersection-observer": "^9.4.4",
    "recharts": "^2.7.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash": "^4.14.195",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsc-files": "^1.1.3",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "src/**/*.+(js,ts,tsx)": [
      "eslint -c ./dapps/reports/.eslintrc.json -cache --fix",
      "tsc-files --noEmit"
    ],
    "src/**/*.{js,ts,tsx,css,md,json}": [
      "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write"
    ]
  }
}
